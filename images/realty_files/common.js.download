// Header sticky
var lastScrollTop = 0;
$(".scroll-search").fadeOut();
window.addEventListener('scroll', (event) => {
    try {
        var sticky = jQuery('header');
        var scroll = jQuery(window).scrollTop();
        const bb = document.querySelector('.footer-form-area').offsetTop;
        var st = window.pageYOffset || document.documentElement.scrollTop;

        if (st > bb) {
            // $(".scroll-search").fadeOut();
        }
        else if (st > lastScrollTop) {
            sticky.removeClass('sticky');
            $(".banner").removeAttr("style");
            $(".scroll-search").fadeIn();
        }
        else {
            if (scroll >= 200) {
                sticky.addClass('sticky');
                //$(".banner").css("margin-top", "0px");
                // $(".scroll-search").fadeOut();
            }
            else {
                sticky.removeClass('sticky');
                //$(".banner").removeAttr("style");
                //  $(".scroll-search").fadeIn();
            }
        }

        lastScrollTop = st <= 0 ? 0 : st;
    } catch (e) {

    }

}, false);


function appendUTMParameters(container) {
    const params = new URLSearchParams(window.location.search);
    var aUTM = [];
    for (const [name, value] of params)
        "utm_source" == name.toLowerCase() && aUTM.push("utm_source=" + value),
            "utm_medium" == name.toLowerCase() && aUTM.push("utm_medium=" + value), "utm_campaign" == name.toLowerCase() && aUTM.push("utm_campaign=" + value),
            "utm_content" == name.toLowerCase() && aUTM.push("utm_content=" + value), "utm_term" == name.toLowerCase() && aUTM.push("utm_term=" + value),
            "utm_company" == name.toLowerCase() && aUTM.push("utm_company=" + value), "utm_adset" == name.toLowerCase() && aUTM.push("utm_adset=" + value),
            "utm_creative" == name.toLowerCase() && aUTM.push("utm_creative=" + value), "utm_country" == name.toLowerCase() && aUTM.push("utm_country=" + value),
            "utm_language" == name.toLowerCase() && aUTM.push("utm_language=" + value), "utm_campaign_id" == name.toLowerCase() && aUTM.push("utm_campaign_id=" + value),
            "utm_adset_id" == name.toLowerCase() && aUTM.push("utm_adset_id=" + value), "utm_ad_id" == name.toLowerCase() && aUTM.push("utm_ad_id=" + value), "utm_adgroup" == name.toLowerCase() && aUTM.push("utm_adgroup=" + value);
    if (aUTM.length > 0) {
        var e, a = aUTM.join("&");
        $(container).find('a').each(function () {
            var href = $(this).attr('href');
            if (href.indexOf("?") < 0) {
                var hashval = "";
                if (href.indexOf('#') > 0) {
                    hashval = href.substring(href.indexOf("#") + 1);
                    href = href.split('#')[0];
                }
                href = href.indexOf("?") > 0 ? href + "&" + a : href + "?" + a;
                if (hashval.length > 0) {
                    href = href + "#" + hashval;
                }
                $(this).attr('href', href);
            }
        });
    }
}
