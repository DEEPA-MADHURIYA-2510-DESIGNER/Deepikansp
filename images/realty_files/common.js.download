    const cursorHalfHeight = cursor.outerHeight() / 3;
    const cursorHalfWidth = cursor.outerWidth() / 3;

    cards.each((_, item) => {
        $(item).on("mouseenter", function () {
            cursor.addClass("active");
            $(this).addClass("active");
        });
        $(item).on("mouseleave", function () {
            cursor.removeClass("active");
            $(this).removeClass("active");
        });
        $(item).mousemove(function (event) {
            cursor.css({
                top: event.pageY - cursorHalfHeight,
                left: event.pageX - cursorHalfWidth
            });
        });
    });
