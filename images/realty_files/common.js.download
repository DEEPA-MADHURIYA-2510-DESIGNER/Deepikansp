    const communitySec = document.querySelectorAll('.community-slide-box');
    function tabfunction() {
        for (i = 0; i < communitySec.length; i++) {
            communitySec[i].addEventListener('mouseover', tabify, true);
        }
    }
    function clearclass() {
        for (i = 0; i < communitySec.length; i++) {
            communitySec[i].classList.remove('selected');
        }
    }
    function tabify(e) {
        clearclass()
        this.classList.add('selected');
    }
    if (communitySec.length > 0) {
        tabfunction();
    }
